package inheritanceExam;

public class AnimalExam {

	public static void main(String[] args) {
		Animal ani = new Animal();
		Cat cat = new Cat();
		Dog dog = new Dog();
		Bird bird = new Bird();
		
	ani.cry();
	cat.cry();
	dog.cry();
	bird.cry();
	
	bird.fly();
	
	cat.jumping();
	dog.runnig();
	
	
	}

}
