package polymorphism.exam;

public class AnimalExam {
	public static void main(String[] args) {
		Animal ani = new Animal();
		Dog dog = new Dog();
		Cat cat = new Cat();
		Bird bird = new Bird();
		
		ani.cry();
		dog.cry();
		cat.cry();
		bird.cry();
		
		Animal ac = new Cat();
		ac.cry();
		
		
	}
}
