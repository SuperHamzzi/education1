package inheritanceExam;

public class AnimalExam {

	public static void main(String[] args) {
		Animal ani = new Animal();
		Cat cat = new Cat();
		Dog dog = new Dog();
		Bird bird = new Bird();
		Bird bird2 =new Bird("두눈","부리","독수리","양날개");
	ani.cry();
	cat.cry();
	dog.cry();
	bird.cry();
	System.out.println(bir2);
	
	
	}

}
