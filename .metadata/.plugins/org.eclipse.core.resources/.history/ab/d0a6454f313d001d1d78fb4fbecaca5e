package runner;

import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Scanner sc =  new Scanner(System.in);
		String a = sc.next().toLowerCase();
		
		int count = 0;
		char b = sc.next().charAt(0);
		for(int i=0; i<a.length();i++) {
			if(b == a.charAt(i)) {
				count++;
			}
		}
		System.out.println(count);

	}

}
