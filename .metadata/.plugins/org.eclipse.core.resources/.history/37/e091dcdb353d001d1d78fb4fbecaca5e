package arrayExam;

public class LottoExam {

	public static void main(String[] args) {
		// 초기값 설정 : 0 ~ 9
		// 섞기 -> random,  10
		// 자리바꿈 => 임시 변수(temp)
	//0~9까지 담고 있을 공간
		int[] numArray = new int[10];
		
		for(int i=0; i<numArray.length; i++) {
			numArray[i] = i; //배열에 0~ 9까지 순차적으로 담김
		}
		System.out.println(numArray);
				
	
	
	
	
	
	
	
	}
}
