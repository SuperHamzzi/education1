package polymorphism.exam;

public class AnimalExam {
	public static void main(String[] args) {
		Animal ani = new Animal();
		Dog dog = new Dog();
		Cat cat = new Cat();
		Bird bird = new Bird();
		
		ani.cry();
		dog.cry();
		dog.running();
		cat.cry();
		cat.jump();
		bird.cry();
		System.out.println();
		Animal ac = new Cat();
		Animal ad = new Dog();
		Animal ab = new Bird();
		
		
		ac.cry();
		ad.cry();
		ab.cry();
		
		
	}
}
